{
  "name": "ts-react-redux-starter-kit",
  "version": "1.0.0",
  "description": "Typescript react-redux starter kit",
  "author": "D. Hristov <dean.hristov89@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "Typescript",
    "React",
    "Redux",
    "Saga",
    "starter kit"
  ],
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --config build/webpack.dev.js --env.NODE_ENV=development --watch",
    "run:dev": "node_modules/.bin/webpack-dashboard -t '[DEV]: e-shopping' -- webpack-dev-server --config build/webpack.dev.js --env.NODE_ENV=development --watch",
    "build:prod": "node_modules/.bin/webpack --config=build/webpack.prod.js --env.NODE_ENV=production",
    "test": "echo do not have any tests yet && npm run ts-lint",
    "test:watch": "node_modules/.bin/jest -c build/jest.config.js --watch",
    "test:coverage": "node_modules/.bin/jest -c build/jest.config.js --coverage",
    "test:coverage:watch": "node_modules/.bin/jest -c build/jest.config.js --coverage --watch",
    "test:watch:all": "node_modules/.bin/jest -c build/jest.config.js --watchAll",
    "ts-lint": "node_modules/.bin/tslint -c tslint.json 'src/**/*.ts{,x}'",
    "ts-lint:fix": "node_modules/.bin/tslint --fix -c tslint.json 'src/**/*.ts{,x}'"
  },
  "devDependencies": {
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "tslint-eslint-rules": "^5.4.0",
    "connected-react-router": "^6.8.0",
    "dotenv": "^6.2.0",
    "es6-shim": "^0.35.5",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "i18next": "^19.6.2",
    "jest": "^26.1.0",
    "node-sass": "^4.14.1",
    "redux-logger": "^3.0.6",
    "source-map-loader": "^1.0.1",
    "ts-loader": "^8.0.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.11.0",
    "whatwg-fetch": "^3.2.0"
  },
  "dependencies": {
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@babel/preset-env": "^7.10.4",
    "@types/moment": "^2.13.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-logger": "^3.0.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux-saga": "^1.1.3",
    "redux": "^4.0.5",
    "formik": "^2.1.5",
    "css-loader": "^3.6.0",
    "moment": "^2.27.0",
    "react-i18next": "^11.7.0",
    "react-router-dom": "^5.2.0",
    "ts-map": "^1.0.3"
  }
}
